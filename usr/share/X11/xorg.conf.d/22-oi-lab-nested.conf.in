Section "Device"
    MatchSeat "@NESTED_SEAT_NAME@"
    Identifier "Nested Device @NESTED_PCI_SLOT@"
    Driver "nested"
    Option "Display" ":@NESTED_DISPLAY_NUMBER@"
EndSection

Section "Screen"
    MatchSeat "@NESTED_SEAT_NAME@"
    Identifier "Nested Screen @NESTED_OUTPUT@ @NESTED_PCI_SLOT@"
    Device "Nested Device @NESTED_PCI_ADDRESS@"
    DefaultDepth 16
    Option "Output" "@NESTED_OUTPUT@"
EndSection