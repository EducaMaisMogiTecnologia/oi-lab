Section "Device"
    MatchSeat "seat-@SEAT_NAME_LVDS@"
    Identifier "Nested Device @PCI_ADDRESS@"
    Driver "nested"
    Option "Display" "@PCI_DISPLAY@"
EndSection

Section "Screen"
    MatchSeat "seat-@SEAT_NAME_LVDS@"
    Identifier "Nested Screen LVDS @PCI_ADDRESS@"
    Device "Nested Device @PCI_ADDRESS@"
    DefaultDepth 16
    Option "Output" "LVDS"
EndSection